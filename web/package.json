    "grunt-contrib-watch": "^1.1.0",
    "grunt-karma": "^4.0.2",
    "html-loader": "^4.2.0",
    "jstify": "^0.14.0",
    "karma": "^6.4.2",
    "karma-browserify": "^8.1.0",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chrome-launcher": "^3.2.0",
