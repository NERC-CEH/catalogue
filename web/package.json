{
    "name": "web",
    "description": "UKCEH Metadata Catalogue",
    "keywords": [],
    "author": "UKCEH Environmental Data Science Group",
    "license": "GPL-2.0-or-later",
    "private": "true",
    "scripts": {
        "build-css": "webpack --config webpack.scss.js --mode production",
        "build-dev": "webpack --mode development",
        "build-prod": "webpack --mode production",
        "lint": "eslint .",
        "standard": "standard",
        "standard-fix": "standard --fix",
        "test": "karma start --single-run",
        "test-server": "concurrently 'webpack --watch' 'karma start'",
        "watch": "webpack --mode development --watch"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es2021": true,
            "jasmine": true
        },
        "extends": [
            "standard"
        ],
        "ignorePatterns": [
            "dist/*"
        ],
        "parser": "@babel/eslint-parser",
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "plugins": [
            "jasmine"
        ]
    },
    "standard": {
        "globals": [
            "afterEach",
            "beforeEach",
            "describe",
            "expect",
            "it",
            "jasmine",
            "spyOn",
            "xdescribe",
            "xit"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.22.15",
        "@babel/eslint-parser": "^7.22.15",
        "@babel/preset-env": "^7.22.15",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^8.2.3",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "eslint": "^8.48.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-jasmine": "^4.1.3",
        "karma": "^6.4.2",
        "karma-babel-preprocessor": "^8.0.2",
        "karma-chrome-launcher": "^3.2.0",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-ajax": "^0.1.13",
        "karma-junit-reporter": "^2.0.1",
        "karma-webpack": "^5.0.0",
        "mini-css-extract-plugin": "^2.9.1",
        "postcss-loader": "^8.1.1",
        "puppeteer": "^22.4.1",
        "sass": "^1.79.4",
        "sass-loader": "^16.0.2",
        "standard": "^17.1.0",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-remove-empty-scripts": "^1.0.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.6.0",
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-brands-svg-icons": "^6.4.2",
        "@fortawesome/free-regular-svg-icons": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "air-datepicker": "^3.4.0",
        "backbone": "^1.5.0",
        "bootstrap": "^5.3.3",
        "bootstrap-colorpicker": "^2.5.3",
        "deparam.js": "^3.0.16",
        "dropzone": "^6.0.0-beta.2",
        "filesize": "^10.0.12",
        "jquery": "^3.7.1",
        "jquery-ui": "^1.13.1",
        "leaflet": "^1.9.4",
        "leaflet-draw": "^1.0.4",
        "leaflet.markercluster": "^1.5.3",
        "sweetalert2": "^11.7.27",
        "underscore": "^1.13.6"
    }
}
