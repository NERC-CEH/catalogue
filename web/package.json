  "license": "GPL-2.0-or-later",
  "private": "true",
  "browser": {
    "./dist/main.bundle.js": "./scripts/main.js"
  },
  "scripts": {
    "build": "grunt babel uglify",
    "watch": "webpack --mode development",
    "test": "grunt karma",
    "test-server": "concurrently 'webpack --watch' 'karma start'",
    "test-watch": "karma start",
    "lint": "eslint ."
  },
  "babel": {
    "presets": [
