    "extends": [
      "standard"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-new": 0,
      "no-undef": 0,
      "prefer-regex-literals": 0,
      "quotes": 0
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
