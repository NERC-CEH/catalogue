    "babel-register": "^6.26.0",
    "babelify": "^10.0.0",
    "brfs": "^2.0.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
