<div class="row">
  <div class="col-md-3">
    <label for="input-schemaId">Id</label>
  </div>
  <div class="col-md-9">
    <input data-name="id" id="input-schemaId" class="editor-input" value="<%= data.id %>">
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-fieldName">Field Name</label>
  </div>
  <div class="col-md-9">
    <input data-name="fieldName" id="input-fieldName" class="editor-input" value="<%= data.fieldName %>">
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-title">Title</label>
  </div>
  <div class="col-md-9">
    <input data-name="title" id="input-title" class="editor-input" value="<%= data.title %>">
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-format">Format</label>
  </div>
  <div class="col-md-9">
    <input data-name="format" id="input-format" class="editor-input" value="<%= data.format %>">
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-fieldDescription">Field Description</label>
  </div>
  <div class="col-md-9">
    <textarea data-name="fieldDescription" id="input-fieldDescription" class="editor-textarea" rows="3"><%= data.fieldDescription %></textarea>
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-missingValues">Missing Values</label>
  </div>
  <div class="col-md-9">
    <input data-name="missingValues" id="input-missingValues" class="editor-input" value="<%= data.missingValues %>">
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-primaryKey">Primary Key</label>
  </div>
  <div class="col-md-9">
    <input data-name="primaryKey" id="input-primaryKey" class="editor-input" value="<%= data.primaryKey %>">
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-constraints">Constraints</label>
  </div>
  <div class="col-md-9">
    <textarea data-name="constraints" id="input-constraints" class="editor-textarea" rows="7"><%= data.constraints %></textarea>
  </div>
</div>
<div class="row">
  <div class="col-md-3">
    <label for="input-measurementUnits">Measurement Units</label>
  </div>
  <div class="col-md-9">
    <input data-name="measurementUnits" id="input-measurementUnits" class="editor-input" value="<%= data.measurementUnits %>">
  </div>
</div>
