{
  "name": "hubbub",
  "description": "Upload files using Hubbub API",
  "private": "true",
  "scripts": {
    "build": "webpack",
    "build-prod": "webpack --mode=production",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "karma start --single-run",
    "test-watch": "babel-watch karma start",
    "watch": "webpack --watch"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jasmine": true
    },
    "extends": [
      "standard"
    ],
    "ignorePatterns": [
      "/dist"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "babel-helpers": "^6.24.1",
    "babel-loader": "^8.2.3",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.7.0",
    "chrome-remote-interface": "^0.31.2",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "jasmine-core": "^4.0.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "puppeteer": "^13.3.1",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "backbone": "1.4.0",
    "bootstrap": "5.1.3",
    "dropzone": "6.0.0-beta.2",
    "filesize": "8.0.7",
    "jquery": "3.6.0",
    "underscore": "1.13.2"
  }
}
